<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.bishe.dao.PicDao">

    <select id="getAll" resultType="com.example.bishe.entity.PicEntity">
        select * from pic
    </select>

    <insert id="add" parameterType="com.example.bishe.entity.PicEntity">
        insert into pic(id, url) VALUES (#{id},#{url})
    </insert>

    <update id="update">
        update pic set url = #{url} where id = #{id}
    </update>

    <delete id="delete">
        delete from pic where id = #{id}
    </delete>

</mapper>